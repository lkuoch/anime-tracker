{
  "name": "anime-tracker",
  "version": "0.0.0",
  "scripts": {
    "dev": "graphql-codegen && vite",
    "build": "tsc && graphql-codegen && vite build",
    "serve": "vite preview",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@elastic/eui": "37.2.0",
    "@reduxjs/toolkit": "1.6.1",
    "graphql": "15.5.1",
    "lodash": "4.17.21",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "7.2.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.0.1",
    "@graphql-codegen/typescript": "2.0.0",
    "@graphql-codegen/typescript-document-nodes": "2.0.0",
    "@graphql-codegen/typescript-operations": "2.0.1",
    "@types/graphql": "14.5.0",
    "@types/jest": "27.0.1",
    "@types/lodash": "4.14.172",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@types/react-redux": "7.1.18",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "prettier": "2.3.2",
    "ts-deep-extract-types": "1.0.1",
    "typescript": "4.3.5",
    "vite": "2.4.4",
    "vite-tsconfig-paths": "3.3.13"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
